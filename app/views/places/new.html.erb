<%= simple_form_for(@place) do |f| %>
  <%= f.input :name %>
  <%= f.input :tax %>
  <%= f.input :control %>
  <%= f.input :reputation %>
  <%= f.input :regime %>
  <%= f.input :position %>
  <%= f.input :continent %>
  <%= f.input :min_income %>
  <%= f.input :description %>
  <%= f.input :thumbnail, as: :file %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <p>Select multiple photos using ctrl+click</p>
  <%= f.submit  %>
<% end %>
